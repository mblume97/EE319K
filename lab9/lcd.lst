   80 0000001A 6811            LDR              R1, [R2]
   81 0000001C F001 0110       AND              R1, #SSI_SR_BSY
   82 00000020 2900            CMP              R1, #0
   83 00000022         ;6) If bit 4 is high, loop back to step 5 (wait for BUSY
                        bit to be low)



