   56 00000000         ; This is a helper function that sends an 8-bit command 
                       to the LCD.
   57 00000000         ; Input: R0  8-bit command to transmit
   58 00000000         ; Output: none
   59 00000000         ; Assumes: SSI0 and port A have already been initialized
                        and enabled
   60 00000000         writecommand
   61 00000000         ;; --UUU-- Code to write a command to the LCD
   62 00000000         ;1) Read SSI0_SR_R and check bit 4,
   63 00000000         Check
   64 00000000 4A0F            LDR              R2, =SSI0_SR_R
   65 00000002 6811            LDR              R1, [R2]
   66 00000004 F001 0110       AND              R1, #SSI_SR_BSY
   67 00000008 2900            CMP              R1, #0
   68 0000000A         ;2) If bit 4 is high, loop back to step 1 (wait for BUSY
                        bit to be low)
   69 0000000A D1F9            BNE              Check
   70 0000000C         ;3) Clear D/C=PA6 to zero
   71 0000000C 4A0D            LDR              R2, =DC
   72 0000000E F04F 0300       MOV              R3, #DC_COMMAND
   73 00000012 6013            STR              R3, [R2]
   74 00000014         ;4) Write the command to SSI0_DR_R
   75 00000014 4A0C            LDR              R2, =SSI0_DR_R
   76 00000016 6010            STR              R0, [R2]
   77 00000018         ;5) Read SSI0_SR_R and check bit 4, 
   78 00000018         Check2
